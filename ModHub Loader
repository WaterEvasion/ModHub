-- Scripts:

local function APPRAI_fake_script() -- Uicorner. 
	local script = Instance.new('Script', Uicorner)

end
coroutine.wrap(APPRAI_fake_script)()
local function NRXA_fake_script() -- KeyCheck.LocalScript 
	local script = Instance.new('LocalScript', KeyCheck)

	local function KeyAccepted(val, key)
		if val == true and key == "IH70SadUy6sdFTRM" then
			script.Parent.LdnTxt.Visible = true
			script.Parent.LdnTxt.Text = "Checking If Game Supported..."
			task.wait(2)
			if game.GameId == game.GameId then
				script.Parent.LdnTxt.Text = "Game Supported!"
				task.wait(1)
				script.Parent.LdnTxt.Text = "Loading Right 2 fight GUI"
				task.wait(5)
				script.Parent.Visible = false
				script.Parent.Parent.Right2FightMenu.Visible = true
			else
				script.Parent.LdnTxt.Text = "Game Not Supported!"
				task.wait(3)
				script.Parent.Parent:Destroy()
			end
		end
	end
	
	script.Parent.AcceptBTN.MouseButton1Down:Connect(function()
		if script.Parent.KeyBox.Text == "IH70SadUy6sdFTRM" then
			script.Parent.KeyBox.Text = "Correct Key!"
			script.Parent.AcceptBTN.Visible = false
			task.wait(1)
			script.Parent.KeyBox.Visible = false
			script.Parent.TextLabel.Visible = false
			KeyAccepted(true, "IH70SadUy6sdFTRM")
		else
			script.Parent.KeyBox.Text = "Invalid Key!"
		end
	end)
end
coroutine.wrap(NRXA_fake_script)()
local function SCMZFEG_fake_script() -- CustomStyles.LocalScript 
	local script = Instance.new('LocalScript', CustomStyles)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent.MenuContents.CustomStyles.Visible = true
	end)
end
coroutine.wrap(SCMZFEG_fake_script)()
local function XIKIJCE_fake_script() -- Changeto.LocalScript 
	local script = Instance.new('LocalScript', Changeto)

	script.Parent.MouseButton1Down:Connect(function()
		if script.Parent.Text == "Brawler" then
			script.Parent.Text = "Rush" 
		elseif script.Parent.Text == "Rush" then
			script.Parent.Text = "Beast"
		elseif script.Parent.Text == "Beast" then
			script.Parent.Text = "Brawler" 
		end
	end)
end
coroutine.wrap(XIKIJCE_fake_script)()
local function SPQIRDG_fake_script() -- CustomChangeto.LocalScript 
	local script = Instance.new('LocalScript', CustomChangeto)

	local AllCustomStyles = {"Draganaga", "TigerLegend", "Dragon", "nagaraja", "BloodSucker", "Parker", "Vulcan"}
	local CurrentStyleNumber = 1
	
	script.Parent.MouseButton1Down:Connect(function()
		if CurrentStyleNumber == #AllCustomStyles then
			CurrentStyleNumber = 1
		else
			CurrentStyleNumber += 1
		end
		script.Parent.Text = AllCustomStyles[CurrentStyleNumber]
	end)
end
coroutine.wrap(SPQIRDG_fake_script)()
local function LYXH_fake_script() -- Apply.LocalScript 
	local script = Instance.new('LocalScript', Apply)

	script.Parent.MouseButton1Up:Connect(function()
		script.Parent.Visible = false
		task.wait(3)
		if script.Parent.Parent.CustomChangeto.Text == "nagaraja" then
			local styleToChange = script.Parent.Parent.Changeto.Text
			local styleToChangeTo = "नागराज"
	
			for i, v in pairs(game.ReplicatedStorage.Styles:FindFirstChild(styleToChange):GetChildren()) do
	
				v:Destroy()
	
			end
	
			for i, v in pairs(game.ReplicatedStorage.Styles:FindFirstChild(styleToChangeTo):GetChildren()) do
				v = v:Clone()
				v.Parent = game.ReplicatedStorage.Styles[styleToChange]
	
			end
		elseif script.Parent.Parent.CustomChangeto.Text == "Dragon" then
			local styleToChange = script.Parent.Parent.Changeto.Text
			local styleToChangeTo = "堂島の龍"
	
			for i, v in pairs(game.ReplicatedStorage.Styles:FindFirstChild(styleToChange):GetChildren()) do
	
				v:Destroy()
	
			end
	
			for i, v in pairs(game.ReplicatedStorage.Styles:FindFirstChild(styleToChangeTo):GetChildren()) do
				v = v:Clone()
				v.Parent = game.ReplicatedStorage.Styles[styleToChange]
	
			end
		elseif script.Parent.Parent.CustomChangeto.Text == "TigerLegend" then
			local TigerBrawler = game:GetService("ReplicatedStorage").Styles["\224\164\168\224\164\190\224\164\151\224\164\176\224\164\190\224\164\156"]:Clone()
			TigerBrawler.Name = "TigerBrawler"
			TigerBrawler.Rush1.Value = "龍GTigerDrop"
			TigerBrawler.Rush2.Value = "龍GTigerDrop"
			TigerBrawler.Rush3.Value = "龍GTigerDrop"
			TigerBrawler.Rush4.Value = "龍GTigerDrop"
