coroutine.wrap(LYXH_fake_script)()
local function TDPS_fake_script() -- MenuContents.Enable/Disable 
	local script = Instance.new('LocalScript', MenuContents)

	local UIS = game:GetService("UserInputService")
	local OpenFrame = true
	
	UIS.InputBegan:Connect(function(input, gameProcessedEvent)
		if input.KeyCode == Enum.KeyCode.RightControl then
			if script.Parent.Parent.Visible == OpenFrame and script.Parent.Parent.Parent.KeyCheck.Visible == false then
				if OpenFrame == false then
					OpenFrame = true
				else
					OpenFrame = false
				end
				script.Parent.Parent.Visible = not script.Parent.Parent.Visible
			end
		end
	end)
end
coroutine.wrap(TDPS_fake_script)()
